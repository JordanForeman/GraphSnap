<h2>Profiles</h2>
{{#if profiles}}
	<table class="table table-striped">
		<thead>
			<tr>
				<th>Name</th>
				<th>&nbsp;</th>
				<th>&nbsp;</th>
			</tr>
		</thead>
		<tbody>
	{{#each profiles}}
			<tr>
				<td><a href="Profile/{{_id}}">{{name}}</a></td>
				<td><a class="btn btn-success" href="Profile/CreateNew?ptid={{_id}}">Add Instance</a></td>
				<td><a class="btn btn-default" href="DataPoint/AddField?ptid={{_id}}">Add Field</a></td>
			</tr>
	{{/each}}
		</tbody>
	</table>
	<a href="Profile/CreateNew" class="btn btn-default">Create New</a>
{{else}}
<p>No Profiles found - <a href="Profile/CreateNew">Create A Profile</a></p>
{{/if}}
